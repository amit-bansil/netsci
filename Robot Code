size (640, 640);
smooth ();
   
for (int x=20; x < width; x+=80){
  for(int y=100; y< height; y+=40){
  // if (x<width/2) {
    //fill (255, 0,0);
  //} else{  
    //   fill (0,0,255);
 // }
  // fill ( 255-((x+40)/width)*255, 0,((x+40)/width)+255);
  // fill( 255-x/3,0,0);
  fill(255-x/3,0,255*(x/150));
   
   
  rect(x, y , 60, 40);
  rect(x+5, y+10, 15, 15);
  rect(x+35, y+10, 15, 15);
  line(x+10, y+30, x+40, y+30);
  rect(x+10, y+40, 40, 50);
  }

}
